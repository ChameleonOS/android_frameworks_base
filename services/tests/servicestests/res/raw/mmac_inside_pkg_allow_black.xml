<?xml version="1.0" encoding="utf-8"?>
<policy>

  <!-- Platform dev key with AOSP -->
  <signer signature="" >
    <allow-all />
    <seinfo value="platform" />
    <package name="com.android.frameworks.servicestests.mmac_install_platform" >
      <deny-permission name="android.permission.NOPE_PERM1" />
      <deny-permission name="android.permission.NOPE_PERM2" />
      <seinfo value="insidepackage" />
    </package>
  </signer>

  <!-- Media dev key in AOSP -->
  <signer signature="" >
    <allow-all />
    <seinfo value="media" />
  </signer>

  <package name="com.android.frameworks.servicestests.mmac_install_platform" >
    <allow-all />
    <seinfo value="package" />
  </package>

  <default>
    <allow-all />
    <seinfo value="default" />
  </default>

</policy>
